{
  "QuotationServerUrl": "http://172.17.0.1:8086/",
  
  
  "Application": {
    "Name": "FusionCacheTests",
    "Version": "1.0.0"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq",
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Literate, Serilog.Sinks.Console",
                "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}{NewLine}{Message:lj} {Properties:j}{NewLine}{Exception}{NewLine}"
              }
            },
            {
              "Name": "Seq",
              "Args": {
                "serverUrl": "http://172.17.0.1:5341"
              }
            }
          ]
        }
      }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '/metrics' or RequestPath like '/health' or RequestPath like '/metrics-open'"
        }
      },
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '/favicon%' or RequestPath like '/swagger%' or RequestPath like '/index.html'"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Microsoft.AspNetCore.Mvc": "Information",
        "Microsoft.AspNetCore.Mvc.Internal": "Information",
        "Microsoft.AspNetCore.Authentication": "Information",
        "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Information",
        "Microsoft.AspNetCore.Http.Result.JsonResult": "Information",
        "Microsoft.AspNetCore.Mvc.Infrastructure.ControllerActionInvoker": "Information",
        "Microsoft.AspNetCore.Http.Result.OkObjectResult": "Warning",
        "CorrelationId.CorrelationIdMiddleware": "Warning",
        "ZiggyCreatures.Caching.Fusion": "Debug",
        "ZiggyCreatures.Caching.Fusion.FusionCache": "Debug"
      }
    }
  }
}
